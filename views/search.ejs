            <div class="col-lg-3 col-md-4" style="text-align: center; width: 500px; margin: 0 auto;" >
              <div class="card mb-4 shadow-sm" style="text-align: left;">
                <img src="<%= getPoke.pict %>" class="card-img-top" alt="...">
                <div class="card-body">
                  <h2 class="card-title pricing-card-title"> <%= getPoke.name %> </h2>
                  <ul class="list-unstyled mt-3 mb-4">
                    <li>Weight: <%= getPoke.weight %> </li>
                    <li>Height: <%= getPoke.weight %> </li>
                  </ul>
                </div>
              </div>
            </div>
    
         
